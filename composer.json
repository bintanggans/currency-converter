{
    "name": "dimaslanjaka/currency-converter",
    "description": "PHP Currency Converter tools, based extending of php-curl-class.",
    "homepage": "https://github.com/dimaslanjaka/currency-converter",
    "license": "MIT",
    "keywords": [
        "php", "curl", "class", "api", "api-client", "client", "framework", "http", "http-client", "http-proxy", "json",
        "php-curl", "php-curl-library", "proxy", "requests", "restful", "web-scraper", "web-scraping", "web-service",
        "xml", "currency-converter", "currency"
    ],
    "authors": [
        {
            "name": "Dimas Lanjaka"
        }
    ],
    "require": {
        "php": ">=5.3",
        "ext-curl": "*",
        "php-curl-class/php-curl-class": "^8.6"
    },
    "require-dev": {
        "ext-gd": "*",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Curl\\": "src/Curl/"
        },
        "files": [
            "test/theme/function.php"
        ]
    },
    "version": "1.0.2"
}